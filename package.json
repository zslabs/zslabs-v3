    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "type:check": "tsc --project tsconfig.json --pretty --noEmit",
    "postinstall": "husky install"
  },
  "overrides": {
    "next-contentlayer": {
