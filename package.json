{
  "name": "zslabs-v3",
  "version": "0.0.0",
  "author": {
    "name": "Zach Schnackel",
    "email": "info@zslabs.com",
    "url": "https://zslabs.com"
  },
  "private": true,
  "browserslist": [
    "last 2 versions and > 0.25%",
    "not dead"
  ],
  "scripts": {
    "prepare": "husky && panda codegen",
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "build:icons": "tsx scripts/icons.ts",
    "build:colors": "tsx scripts/colors.ts",
    "build:assets": "npm run build:icons && npm run build:colors",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "type:check": "tsc --project tsconfig.json --pretty --noEmit",
    "postinstall": "husky install"
  },
  "overrides": {
    "next-contentlayer": {
      "next": "$next"
    }
  },
  "dependencies": {
    "@fontsource-variable/jetbrains-mono": "5.0.21",
    "@fontsource-variable/manrope": "5.0.20",
    "@iconify/tools": "4.0.4",
    "@pandacss/eslint-plugin": "0.1.5",
    "@radix-ui/colors": "3.0.0",
    "@radix-ui/react-tooltip": "1.0.7",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/fs-extra": "11.0.4",
    "@types/node": "20.14.2",
    "@types/react": "18.3.3",
    "@types/rss": "0.0.32",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@vercel/analytics": "1.3.1",
    "contentlayer": "0.3.4",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react-perf": "3.3.2",
    "framer-motion": "11.2.10",
    "fs-extra": "11.2.0",
    "immer": "10.1.1",
    "mini-svg-data-uri": "1.4.4",
    "nanospinner": "1.1.0",
    "next": "14.2.3",
    "next-contentlayer": "0.3.4",
    "prettier": "3.3.1",
    "react": "18.3.1",
    "react-aria-components": "1.2.1",
    "react-dom": "18.3.1",
    "rehype-pretty-code": "0.13.2",
    "rehype-slug": "6.0.0",
    "remark-code-import": "1.2.0",
    "rss": "1.2.2",
    "sharp": "0.33.4",
    "shiki": "1.6.3",
    "tsx": "4.13.2",
    "typescript": "5.4.5",
    "unist-util-visit": "5.0.0",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@pandacss/dev": "0.40.1",
    "@svgr/webpack": "8.1.0",
    "husky": "9.0.11",
    "nano-staged": "0.8.0"
  }
}
